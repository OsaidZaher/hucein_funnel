{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/freeguide/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/freeguide",
      "dest": "/index.html"
    },
    {
      "src": "/thank-you",
      "dest": "/index.html"
    },
    {
      "src": "/((?!api).*)",
      "dest": "/index.html"
    }
  ]
}
